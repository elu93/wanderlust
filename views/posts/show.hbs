<div class="post-container">
  <div class="post-main">
    <div>
      <div class="post-title">
        <p class="first-post-p">{{post.name}}</p>
      </div>
      <div class="post-description">
        <h5>{{post.postDescription}}</h5>
      </div>
      <img class="image-post" src="{{post.image}}" alt="">
      <div class="author">
        <p>By: {{user.firstName}} {{user.lastName}}</p>
      </div>
      <div> {{post.body}}</div>
      <a class="button warning" href="/users/{{userId}}/journals/{{journal._id}}/posts/{{post._id}}/delete">Delete</a>
    </div>
  </div>
</div>