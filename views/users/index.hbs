<div>
  <div class="main">
    {{#each users}}
      <div class="profiles">
          <a href="/users/{{this._id}}"><img class="pro-pics" src="{{this.photoUrl}}" alt="{{this.username}}'s profile photo"></a> 
          <div>
        {{this.firstName}} {{this.lastName}}
      </div>
      </div>
    {{/each}}
  </div>

  <div>
    <a href="/users/new">
      New User
    </a>
  </div>
</div>