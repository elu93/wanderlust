<div class="row">
  {{#each users}}
  <div class="col s12 m6 offset-m3">
    <div class="card">
      <div class="card-image">
        <img class="pro-pics" src="{{this.backgroundImage}}" alt="{{this.username}}'s cover photo">
        <span class="card-title">{{this.firstName}} {{this.lastName}}</span>
      </div>
      <div class="card-action">
        <a class="valign-wrapper" href="/users/{{this._id}}">
          <img class="journal-pro-image" src="{{this.photoUrl}}" alt="{{this.username}}'s cover photo">
          <span class="profile-link">Profile</span>
        </a>
      </div>
    </div>
  </div>
  {{/each}}
</div>